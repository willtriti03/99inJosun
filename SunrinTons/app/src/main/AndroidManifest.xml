<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.jungjune.sunrintons">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Activitys.LoginActivity">
        </activity>
        <activity
            android:name=".Activitys.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.SEARCH" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Activitys.SplashActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Activitys.SignUpActivity" />
        <activity android:name=".Activitys.MyPostsActivity" >
        </activity>
        <activity android:name=".Activitys.GonggoActivity">
        </activity>
        <activity android:name=".Activitys.GonggoRegisterActivity">
        </activity>
        <activity android:name=".Activitys.SettingActivity">
        </activity>
        <activity android:name=".Activitys.ZiownRegisterActivity">
        </activity>
    </application>

</manifest>